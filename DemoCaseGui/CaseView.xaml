              VerticalAlignment="Top"
              HorizontalAlignment="Left"
              Margin="10"
              >
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="auto"/>
                                            </Grid.RowDefinitions>
                                            <TextBlock Text="CONTROL" Style="{StaticResource FieldTitle}"/>
                                            <Grid Grid.Row="1" Margin="5">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <StackPanel Grid.Column="0"
                              >
                                                    <Button Margin="5" Content="START" IsEnabled="{Binding IsStatus}" Command="{Binding StartInverterCommand}" Padding="0" Width="90" Height="20" Grid.Column="3" Grid.Row="0">
                                                        <!--<i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="Click">
                                                            <prism:InvokeCommandAction IsEnabled="{Binding IsConnected}" Command="{Binding StartInverterCommand}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>-->
                                                    </Button>
                                                    <Button Margin="5" Content="STOP" IsEnabled="{Binding IsStatus}" Command="{Binding StopInverterCommand}" Padding="0" Width="90" Height="20" Grid.Column="3" Grid.Row="0">
                                                        <!--<i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="Click">
                                                            <prism:InvokeCommandAction IsEnabled="{Binding IsConnected}" Command="{Binding StopInverterCommand}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>-->
                                                    </Button>
                                                    <Button Margin="5" Content="FWD" Padding="0" Width="90" IsEnabled="{Binding IsStatus}" Command="{Binding FWDInverter}" Height="20" Grid.Column="3" Grid.Row="0">
                                                        <!--<i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="Click">
                                                            <prism:InvokeCommandAction IsEnabled="{Binding IsConnected}" Command="{Binding FWDInverter}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>-->
                                                    </Button>
                                                    <Button Margin="5" Content="REV" Padding="0" IsEnabled="{Binding IsStatus}" Command="{Binding REVInverter}" Width="90" Height="20" Grid.Column="3" Grid.Row="0">
                                                        <!--<i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="Click">
                                                            <prism:InvokeCommandAction IsEnabled="{Binding IsConnected}" Command="{Binding REVInverter}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>-->
                                                    </Button>
                                                </StackPanel>
                                                <StackPanel Grid.Column="1">
                                                    <StackPanel Orientation="Horizontal">
                                                        <components:Indicator Grid.Row="3" Grid.Column="0" Width="15" Height="15" Margin="30 5 10 0" State="{Binding ButtonStartup, Mode=TwoWay}"/>
                                                        <TextBlock Margin="0 5 10 0" Grid.Row="2" Grid.Column="0" Text="On" HorizontalAlignment="Center"/>
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal">
                                                        <components:Indicator Grid.Row="3" Grid.Column="0" Width="15" Height="15" Margin="30 5 10 0" State="{Binding ButtonStop, Mode=TwoWay}"/>
                                                        <TextBlock Margin="0 5 10 0" Grid.Row="2" Grid.Column="0" Text="Off" HorizontalAlignment="Center"/>
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal">
                                                        <components:Indicator Grid.Row="3" Grid.Column="0" Width="15" Height="15" Margin="30 5 10 0" State="{Binding MotorForward, Mode=TwoWay}"/>
                                                        <TextBlock Margin="0 5 10 0" Grid.Row="2" Grid.Column="0" Text="Fwd" HorizontalAlignment="Center"/>
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal">
                                                        <components:Indicator Grid.Row="3" Grid.Column="0" Width="15" Height="15" Margin="30 5 10 0" State="{Binding MotorReverse, Mode=TwoWay}"/>
                                                        <TextBlock Margin="0 5 10 0" Grid.Row="2" Grid.Column="0" Text="Rev" HorizontalAlignment="Center"/>
                                                    </StackPanel>
                                                </StackPanel>
                                            </Grid>
                                            <Grid Grid.Row="2" Margin="10">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition/>
                                                    <RowDefinition/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <TextBlock Text="Speed SP" Margin="10 10 10 0" HorizontalAlignment="Left"/>
                                                <StackPanel Grid.Column="1" Margin="10 10 10 0" Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Center">
                                                    <TextBlock Text="{Binding MotorSetpoint, Mode=TwoWay}" HorizontalAlignment="Center"/>
                                                    <TextBlock Text=" RPM"/>
                                                </StackPanel>
                                                <TextBlock Text="Speed PV"  Grid.Column="0" Grid.Row="1" HorizontalAlignment="Left" Margin="10 10 10 0" />
                                                <StackPanel Margin="10 10 10 0"  Grid.Column="1" Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
                                                    <TextBlock Text="{Binding MotorSpeed, Mode=TwoWay}" HorizontalAlignment="Center"/>
                                                    <TextBlock Text=" RPM"/>
                                                </StackPanel>
                                                <TextBlock Grid.Column="0" Grid.Row="2" Margin="10 10 10 0"  Text="Write Setpoint" HorizontalAlignment="Left"/>
                                                <StackPanel Grid.Column="1" Grid.Row="2" Margin="10 10 10 0" Orientation="Horizontal" HorizontalAlignment="Center">
                                                    <TextBox Margin="5 0" Width="30" TextAlignment="Center" Text="{Binding MotorSetpointWrite, Mode=TwoWay}" HorizontalAlignment="Center"/>
                                                    <Button Margin="5 0" Content="OK" IsEnabled="{Binding IsStatus}" Command="{Binding MotorSetpointOKCommand}" Padding="0" Width="35" Height="20">
                                                        <!--<i:Interaction.Triggers>
                                                        <i:EventTrigger EventName="Click">
                                                            <prism:InvokeCommandAction IsEnabled="{Binding IsConnected}" Command="{Binding MotorSetpointOKCommand}" />
                                                        </i:EventTrigger>
                                                    </i:Interaction.Triggers>-->
                                                    </Button>
                                                </StackPanel>
                                            </Grid>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </Grid>
                        </materialDesign:Card>
                        <materialDesign:Card Grid.Row="3" Margin="10">
                            <Grid Grid.Row="1">
                                <lvc:CartesianChart
SyncContext="{Binding Sync}"
Series="{Binding Series}"
XAxes="{Binding XAxes}">
                                </lvc:CartesianChart>
                            </Grid>
                        </materialDesign:Card>
                    </Grid>
                </Grid>
                <Ruler:Ruler10 HorizontalAlignment="Left" Margin="874,554,0,0" Grid.Row="1" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
                    <Ruler:Ruler10.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="90"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Ruler:Ruler10.RenderTransform>
                </Ruler:Ruler10>
            </Grid>

        </materialDesign:DialogHost>



    </busyIndicator:BusyMask>
</UserControl>